{
  "description": "Mustache templating for the Yii Framework.",
  "homepage": "https://dev.belin.io/yii2-mustache",
  "license": "MIT",
  "name": "cedx/yii2-mustache",
  "type": "yii2-extension",
  "authors": [
    {"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
  ],
  "autoload": {
    "psr-4": {"yii\\mustache\\": "lib/"}
  },
  "autoload-dev": {
    "psr-4": {"yii\\mustache\\": "test/"}
  },
  "config": {
    "fxp-asset": {"enabled": false},
    "optimize-autoloader": true
  },
  "keywords": [
    "mustache",
    "renderer",
    "templating",
    "view",
    "yii2"
  ],
  "repositories": [
    {"type": "composer", "url": "https://asset-packagist.org"}
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pcre": "*",
    "mustache/mustache": "^2.12.0",
    "psr/log": "^1.1.2",
    "yiisoft/yii2": "^2.0.30"
  },
  "require-dev": {
    "cedx/coveralls": "^12.0.0",
    "cedx/phpunit-expect": "^0.8.0",
    "consolidation/robo": "^2.0.0",
    "phpstan/phpstan": "^0.11.19"
  },
  "scripts": {
    "coverage": "robo coverage",
    "test": "robo test"
  },
  "support": {
    "docs": "https://dev.belin.io/yii2-mustache/api",
    "issues": "https://github.com/cedx/yii2-mustache/issues"
  }
}
