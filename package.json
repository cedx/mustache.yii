{
  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://bitbucket.org/cedx/mustache.yii/issues",
  "description": "Mustache templating for Yii Framework.",
  "homepage": "https://packagist.org/packages/cedx/yii2-mustache",
  "license": "MIT",
  "name": "yii2-mustache",
  "private": true,
  "version": "0.3.0",
  "devDependencies": {
    "del": "^1.1.1",
    "docgen-js": "^0.0.1",
    "gulp": "^3.8.11",
    "gulp-david": "^0.2.1",
    "gulp-jshint": "^1.10.0",
    "gulp-load-plugins": "^0.9.0",
    "gulp-zip": "^3.0.2"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": "nofunc",
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "scripts": {
    "test": "gulp test"
  },
  "yuidoc": {
    "logo": "http://dev.belin.io/mustache.yii/favicon.ico",
    "name": "Mustache.yii",
    "url": "https://packagist.org/packages/cedx/yii2-mustache",
    "options": {
      "extension": ".php",
      "outdir": "doc/api",
      "paths": "lib",
      "tabtospace": 2,
      "theme": "spacelab"
    }
  }
}
